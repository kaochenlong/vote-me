<!DOCTYPE html>
<html>
  <head>
    <title>VoteMe</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'frontend/application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render "shared/nav" %>
    <div class="main-container">
      <%= render "shared/flash" %>
      <%= yield %>
    </div>
  </body>
</html>
